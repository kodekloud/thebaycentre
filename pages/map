<script>
    $(document).ready(function() {
        function renderAll (){
            var stores = getStoresList();
            regions = {}
            $.each( stores , function( key, val ) {
                if(val.svgmap_region != null && typeof(val.svgmap_region)  != 'undefined'){
                    // reg = "'"+val.svgmap_region+"' : {tooltip: '<p>TEST</p>'}"
                    
                    obj = {};
                    obj = "{tooltip : 'test'}"
                    regions["stores_x5F_3_x5F_6_2_"] = obj
                    _item = $("#stores_x5F_3_x5F_6_2_")
                }
    
            });
            
           
            console.log(regions)
            
            load_map(regions)
            
        }
        function renderPageData(container, template, collection, type){
            
        }
        
        
    
        function load_map(reg){
            map = $('#mapsvg').mapSvg({
                source: getSVGMapURL(),    // Path to SVG map
                colors: {stroke: '#aaaaaa', selected: -20, hover: "#99CA3C"},
                // disableAll: true,
                // regions: reg,
                regions: {stores_x5F_3_x5F_6_2_ : {tooltip:"test"}}
                tooltipsMode:'custom',
                loadingText: "loading...",
                zoom: true,
                zoomButtons: {'show': true,'location': 'left' },
                pan:true,
                cursor:'pointer',
                responsive:true,
                zoomLimit: [0,10]
            });
        }
        loadMallData(renderAll);  
    })
    
    
    
</script>